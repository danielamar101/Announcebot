
<h1>Login</h1>

<%= form_with scope: :session, url: session_path, local: true do |f| %>
  <p>
    <%= f.label :"Username" %><br/>
    <%= f.text_field :username %>
  </p>

    <%= f.label :"Password" %><br/>
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.submit :"Login" %>
  </p>
<% end %>
